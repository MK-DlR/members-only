<!-- views/partials/footer.ejs -->

    <div class="footer">
        <% if (currentUser && currentUser.membership) { %>
            <a href="/log-out">Log Out</a> | 
            <a href="/membership">Membership</a> |
            <a href="/admin">Admin</a>
        <% } else if (currentUser) { %>
            <a href="/log-out">Log Out</a> | 
            <a href="/membership">Membership</a>
        <% } else { %>
            <a href="/sign-up">Sign Up</a> | 
            <a href="/log-in">Log In</a>
        <% } %>
    </div>

    </body>
</html>